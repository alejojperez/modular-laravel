{
    "name": "alejojperez/modular-laravel",
    "description": "This is my package modular-laravel",
    "keywords": [
        "alejojperez",
        "laravel",
        "modular-laravel"
    ],
    "homepage": "https://github.com/alejojperez/modular-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandro Perez",
            "email": "alejandro.perez.developer@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "ModularLaravel\\ModularLaravel\\": "src",
            "ModularLaravel\\ModularLaravel\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModularLaravel\\ModularLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModularLaravel\\ModularLaravel\\ModularLaravelServiceProvider"
            ],
            "aliases": {
                "ModularLaravel": "ModularLaravel\\ModularLaravel\\Facades\\ModularLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
